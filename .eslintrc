{
  "env": {
    "jquery"  : true,
    "es6"     : true,
    "browser" : true
  },
  "globals": {
    "ga"         : true,
    "AWM"        : true,
    "Materialize": true
  },
  "parser": "babel-eslint",
  "rules": {
    "strict": 0,
    "indent": [2, 2, {"SwitchCase": 1}],
    "no-unused-vars": [1, {"vars" : "all", "args" : "after-used"} ]
  }
}
